//HUNNU DELICIOUS CAKES
#include<iostream>
#define MAXN 21
using namespace std;

int col[ MAXN ][ MAXN ];
int ber[ MAXN ][ MAXN ];
int l,n;
int coled ;
int rear,front;


void dfs(int r , int c)//row and col
{
  int i;
  for()
  return ;
}

int main()
{
  int i,t,cx,cy;
  cin>>t;
  while(t > 0 )
    {
      cin>>l>>n;
      coled = 0;
      for(i = 1 ; i <= n ; ++ i)
	{
	  cin>>cx>>cy;
	  col[ cx ][ cy ] = 1;
	}
      for(i = 1 ; i <= n ; ++ i)
	{
	  for()
	    {
	      ber[ i ][ j ] += ber[ i - 1 ][ j ] + col[ i ][ j ];
	      ber[ i ][ j ] += ber[ i ][ j - 1 ] + col[ i ][ j ];
	    }
	}
	  
      dfs(1,1);
      if(coled == l * l)
	cout<<"YES\n";
      else
	cout<<"NO\n";
      -- t;
    }
  return 0;
}
