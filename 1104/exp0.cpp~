#include<iostream>
#include<string>
using namespace std;

string c[ 2002 ][ 2002 ];

int main()
{
  int m,i,j;
  m = 2000;
  for(i = 1 ; i <= m ; ++ i )
    for(j = 1 ; j <= m ; ++ j)
      c[ i ][ j ] = '%';
  for(i = 1 ; i <= m ; ++ i )
    {
      for(j = 1 ; j <= m ; ++ j)
	cout<<c[ i ][ j ];
      cout<<"\n";
    }
  return 0;
  
}
