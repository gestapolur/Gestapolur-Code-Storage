#include<fstream>
using namespace std;

ofstream cout("upa7.in",ios::binary);

int main()
{
  int i,j;
  cout<<50<<" "<<50<<"\n";
  srand(time(0));
  for( i = 1 ;i <= 50 ; ++ i)
    {
      for( j = 1 ;j <= 50 ; ++ j)
	if(rand() >> 1 )
	  cout<<"S";
	else 
	  cout<<"B";
      cout<<"\n";
    }
  return 0;
}
