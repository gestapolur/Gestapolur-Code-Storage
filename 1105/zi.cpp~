#include<iostream>
#define MAXS 2505
#define MAXN 1005
using namespace std;

int f[ MAXS ][ MAXN ];
int flow;
int n,m;

int main()
{
  int i , minw , maxw;
  while(cin>>n>>m)
    {
      for( i = 1 ; i <= n ; ++ i)
	{
	  cin>>minw>>maxw;
	  for(j = minw ; j <= maxw ; ++ j)
	    w[ i + 1 ][ j ] = 1;
	}
    }
  return 0;
}
