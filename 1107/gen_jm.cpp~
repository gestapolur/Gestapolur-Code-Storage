#include<fstream>
#include<cstdlib>
#include<cstdio>
#define MAXN 20
using namespace std;

ofstream cout("testm.in",ios::binary);

int main()
{
  cout<<"100\n";
  srand( 1000 );
  for(int i = 1 ; i <= 100 ; ++ i)
    {
      n = rand() % MAXN + 1 ; m = rand() % ( n - 1 ) + 1;
      cout<<n<<" "<<m<<"\n";
      for( int j = 1 ; j <= n ; ++ j )
	cout<<rand()%10;
      cout<<"\n";
    }
  return 0;
}
