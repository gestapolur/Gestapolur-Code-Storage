//NEERC 02 C
#include<iostream>
#define MAXN 105
using namespace std;

int tx , ty , n ;
int maxw , mx , my ;
int px[ MAXN ] , py[ MAXN ] ;

bool inside( int sx , int ex , int sy ,int ey , int x  )
{
  if( px[ x ] > sx and px[ x ] < ex and py[ x ] < ey and py[ x ] > sy )
    return true;
  return false;
}

inline int min( int a , int b ){ return a < b ? a : b;}

inline int max( int a , int b ){ return a > b ? a : b;}

