int dblcmp( double d )
{
  if( fabs( d ) < ZERO ) return 0;
  return d > 0 ? 1 : -1 ;
}

double det( double x1 , double y1 , double x2 , double y2 )
{ return x1 * y2 - x2 * y1 ; }

