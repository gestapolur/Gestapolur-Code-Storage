#include<iostream>
using namespace std;

void add( int a[ ] )
{
  a[ 1 ] ++ ; 
  int i = 1 ; 
  while( a[ i ] == 10 )
    {
      a[ i ] = 0 ;
      ++ a[ i + 1 ];
    }
  if( a[ a[ 0 ] + 1 ] > 0 ) ++ a[ 0 ];
  return ;
}

int main()
{
  cout<< ( ( 1 << 0 ) & 15 )<<"\n";
  for( int i = 0 ; i < 8 ; ++ i )
    cout<<( ( 15 >> i ) & 1 );
  cout<<"\n";
  return 0;
}
