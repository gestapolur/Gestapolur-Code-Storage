}


int main()
{
  int n; 
  while( cin >> n )
    {
      if( n == 0 ) break;
      else if( n == 1  ) cout<<"2\n";
      else if( n == 2 ) cout<<"3\n";
      else
        cout<<p[ n ]<<"\n";
    }
  return 0;
}
