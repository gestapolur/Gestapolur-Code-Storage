int p[ MAXN ];
bool a[ MAXP ];

int main()
{
  int i , j , k = 2 ;
  int cnt = 0 ;
  while( cnt <= 22000 )
    {
      while( a[ k ] ) ++ k ;
      cout<<k<<",";
      p[ ++ cnt ] = n;
      i = k ; j = 2 ;
      while( i < 50000000)
        {
          a[ i ] = true;
          i = k * j ;
          ++ j ;
        }
      
    }
