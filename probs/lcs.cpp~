/*
  LCS with record
 */
#include<iostream>
#define MAXN 1002
using namespace std;

int a[ MAXN ] , b[ MAXN ];
int n , m;

void init()
{
  cin>>n;
  for( int i = 1 ; i <= n ; ++ i )
    cin>>a[ i ];
  cin>>m;
  for( int i = 1 ; i <= m ; ++ i )
    cin>>b[ i ];
  return ;
}

int main()
{
  init();

  dp();

  return 0;
}
