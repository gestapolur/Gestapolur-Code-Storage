#include<iostream>
#define MAXN 100006
using namespace std;

long double a[ MAXN ];
bool hash[ MAXN ];
int n;

int main()
{
  int i ;
  while( cin>>n )
    {
      for( i = 1 ; i <= n ; ++ i )
        cin>>a[ i ];
      for( i = 1 ; i <= n ;  )
        {
          if( hash[ i ] ) ++ i ;
          else
            {
              hash[ i ] = true;
              j = i + 1 ;
              while( a[ i ] )
                {
                  
                }
            }
        }
    }
  return 0;
}
